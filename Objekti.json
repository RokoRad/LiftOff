"Flight": {
    "TimeFlown": int (broj sekundi koji si letia),
    "FlySafeScore": double,
    "Drone": {
        "Name": string
    },
    "FlightLocation": {
        "FlightSpot": string (misto),
        "Latitude": double,
        "Longitude": double
    },
    "FlightTime": {
        "FlightStartTime": datetime
    }
}

"TimeLocation": {
    "Location": {
        "Latitude": double,
        "Longitude": double
    },
    "Time": datetime
}

"WeatherData": {
    "TimeLocation": TimeLocation,
    "Humidity": double?,
    "Pressure": double?,
    "Temperature": double?,
    "Max_Temperature": double?,
    "Min_Temperature": double?,
    "Visibility": double?,
    "Cloudiness": double?,
    "UVIndex": double?,
    "WindSpeed": double?,
    "WindDirection": double?,
    "WeatherId": int?,
    "Weather": string,
    "WeatherDescription": string,
    "Units": string ("metric"/"imperial" da znas oces stavljat C/F i km/h/mph)
}

"WeatherRating": {
    "TimeLocation": TimeLocation,
    "TotalRating": double?,
    "ConditionsRating": double?,
    "WindRating": double?,
    "TemperatureRating": double?,
    "AtmosphereRating": double?,
    "VisibilityRating": double?,
    "UVRating": double?,
    "weatherData": WeatherData
}

"User": {
    "Id": guid,
    "UserName": string,
    "TotalTimeFlown": int, *
    "TotalFlights": int, *
    "TotalFlySafeScore": double, *
    "FavoriteFlightTime": string (morning, afternoon, night), *
    "FavoriteFlightSpot": string (misto), *
    "ShowWhereIFly": bool,
    jos neke popratne pizdarije (tebi su samo bitni ovi sa *),
    ti na frontendu tribas uzet TotalTimeFlown, TotalFlights, FavoriteFlightTime i FavoriteFlightSpot ko statove i plus tribas izracunat
    "Average flight time" (TotalTimeFlown / TotalFlights) i "Average FlySafe score" (TotalFlySafeScore / TotalFlights)
}
